      "babel-polyfill"
    ]
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.8.0",
    "bluebird": "^3.3.5",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^2.9.0",
    "grunt": "^1.0.1",
    "grunt-concurrent": "^2.3.0",
    "grunt-env": "^0.4.4",
    "grunt-shell": "^1.3.0",
    "load-grunt-tasks": "^3.5.0",
    "tap-spec": "^4.1.1",
    "tap-xunit": "^1.3.1",
    "time-grunt": "^1.3.0",
    "wd": "^0.4.0"
  }
