{
  "name": "grunt-ava-parallel-tests",
  "version": "1.0.0",
  "description": "Multi level parallel tests with ava and grunt",
  "license": "MIT",
  "repository": "subash-canapathy/grunt-ava-parallel-tests",
  "author": {
    "name": "Subash Canapathy",
    "email": "subash.canapathy@gmail.com",
    "url": "https://github.com/subash-canapathy"
  },
  "scripts": {
    "test": "grunt concurrent:all",
    "ava": "ava",
    "grunt": "grunt",
    "ava-tap": "ava \"src/tests/**/*.js\" --verbose --tap | tap-xunit",
    "lint": "eslint ."
  },
  "keywords": [
    "ava",
    "test",
    "framework",
    "runner",
    "concurrent",
    "parallel",
    "appium"
